{
  "manifest_version": 3,
  "name": "抖音cookie获取",
  "version": "1.0",
  "permissions": [
    "cookies",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "hello_extensions.png"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
